#!/bin/bash

set -e

source "${BASH_SOURCE%/*}/pairup-helper-functions"

main() {
  cat <<...
PairUp Server Info:

  Server Created:    $PAIRUP_STARTED
  VM Address:        $PAIRUP_VMADDR
  Admin User:        $PAIRUP_ADMIN
  Pair User:         $PAIRUP_PAIR

...
}

[ "$0" != "$BASH_SOURCE" ] || main "$@"
